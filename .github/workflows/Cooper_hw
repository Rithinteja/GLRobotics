#region VEXcode Generated Robot Configuration
from vex import *
import urandom

# Brain should be defined by default
brain=Brain()

# Robot configuration code
left_drive_smart = Motor(Ports.PORT1, GearSetting.RATIO_18_1, False)
right_drive_smart = Motor(Ports.PORT2, GearSetting.RATIO_18_1, True)
drivetrain = DriveTrain(left_drive_smart, right_drive_smart, 319.19, 295, 40, MM, 1)
controller_1 = Controller(PRIMARY)
intake_motor = Motor(Ports.PORT6, GearSetting.RATIO_18_1, False)
spin_wheel_motor = Motor(Ports.PORT10, GearSetting.RATIO_18_1, False)


# wait for rotation sensor to fully initialize
wait(30, MSEC)
#endregion VEXcode Generated Robot Configuration

# ------------------------------------------
# 
# 	Project:      VEXcode Project
#	Author:       VEX
#	Created:
#	Description:  VEXcode V5 Python Project
#   
#   Configuration: Controller
#                  intake_motor in Port 6
#                  spin_wheel_motor in Port 10
#                  Drivetrain Motors in Ports 1 & 2
#                  
#
#
#
# ------------------------------------------

# Library imports
from vex import *

# Begin project code

intake_motor.set_stopping(HOLD)
spin_wheel_motor.set_stopping(HOLD)
drivetrain.drive_for(FORWARD, 10, INCHES)
drivetrain.turn_for(RIGHT, 45, DEGREES)
intake_motor.set_timeout(6, SECONDS)
intake_motor.spin(REVERSE)
spin_wheel_motor.set_max_torque(100, PERCENT)
spin_wheel_motor.set_velocity(100, PERCENT)
spin_wheel_motor.set_timeout(4, SECONDS)
spin_wheel_motor.spin(FORWARD)
